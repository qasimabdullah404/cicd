{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": 0,
        "camelcase": 0,
        "no-tabs": 0,
        "no-undef": 0,
        "consistent-return": 0,
        "array-callback-return": 0
    }
}
